    @if (TempData["ToastMsg"] != null)
    {
        <script>
          var success = @((TempData["ToastMsg"] != null).ToString().ToLower());
          if (success == true) {
              var jsonToast = JSON.parse(decodeHtml('@TempData["ToastMsg"]'));
              CreateToasterMessage(jsonToast.MsgTitle, jsonToast.MsgBody, jsonToast.MsgType);
          }
          $(function () {
              $("#tooltip").text($(this).attr('alt'));
          });
        </script>
    }
    @if (Session["CUser"] == null)
    {
        <script>
            $(document).ready(function () {
            var url = '@Url.Action("Login", "Account")';
               location.href = url;
         });
        </script>
